%%Page: 1 1
%%
%% letterhead stuff
%% Harry Payne
%
%  You should be able to print this file on a PostScript printer/queue,
%  and have it produce an STScI logo.

% PostScript commands
%
%	x y moveto		move to (x,y) "pen up"
%	x y lineto		move to (x,y) "pen down"
%	x y r a1 a2 arc		draw arc centered on (x,y), radius r
%				 counterclockwise between angles 
%				 a1 and a2, where angle 0 is up
%	x y r a1 a2 arcn	same as arc except drawn clockwise

/logoFIGURE {  % Draw the graphic
newpath 
5 46.5 moveto
1.5 46.5 lineto
1.5 1.5 lineto
46.5 1.5 lineto
46.5 46.5 lineto
43 46.5 lineto stroke			% box around telescope
newpath
24 46.5 19 0 360 arc stroke		% entrance pupil
newpath
24 46.5 9 0 360 arc stroke		% secondar
newpath
24 -9 23 27.16 152.84 arc stroke	% earth
newpath 
41.09 38.20 moveto
24 28.2 11.25 322 218 arcn
6.91 38.20 lineto stroke		% telescope tube
newpath
24 28.2 11.25 67 14 arcn		% upper right of primary
32.94 47.56 lineto stroke		% ray tangent to secondary
newpath
32.79 44.54 moveto	
24 5 lineto				% tangent from secondary to earth
15.21 44.54 lineto stroke 		% back to secondary
newpath
24 28.2 11.25 113 166 arc 
15.06 47.56 lineto stroke		% ray tangent to secondary
} def

/logoPLOT {  		% Draw the graphic, and put on the words "Space Tel..."
save
0.5 setlinewidth   
32 32 translate 	% Control the absolute positioning
4.5 dup scale 
logoFIGURE
48 1.5 translate
1 1.475 div dup scale
.93 1 scale
/Times-Roman findfont 24 scalefont setfont
.91 dup scale
 57 logoSPACE
 38 logoTEL
 19 logoSCI
 0 logoINST
1 .93 div 1 scale
restore
} def

/logoSPACE { 		% The word "SPACE", scrunched horizontally.
0 exch moveto (S) show
-3.0 0 rmoveto (P) show
-6.0 0 rmoveto (A) show
-4.3 0 rmoveto (C) show
-2.0 0 rmoveto (E) show } def
/logoTEL { 		% The word "TELESCOPE"
0 exch moveto (T) show
-1.00 0 rmoveto (E) show
-2.00 0 rmoveto (L) show
-2.00 0 rmoveto (E) show
-2.00 0 rmoveto (S) show
-2.00 0 rmoveto (C) show
-2.00 0 rmoveto (O) show
-2.00 0 rmoveto (P) show
-2.00 0 rmoveto (E) show } def
/logoSCI { 		% The word "SCIENCE"
0 exch moveto (S) show
-2.00 0 rmoveto (C) show
-3.00 0 rmoveto (I) show
-1.50 0 rmoveto (E) show
-1.00 0 rmoveto (N) show
-2.50 0 rmoveto (C) show
-2.00 0 rmoveto (E) show } def
/logoINST { 		% The word "INSTITUTE"
0 exch moveto (I) show
-2.00 0 rmoveto (N) show
-3.00 0 rmoveto (S) show
-3.00 0 rmoveto (T) show
-1.50 0 rmoveto (I) show
-1.50 0 rmoveto (T) show
-1.00 0 rmoveto (U) show
-1.00 0 rmoveto (T) show
-1.00 0 rmoveto (E) show } def

/logoACKN { 		% The acknowledgement line at the bottom of the page
save
0.5 setlinewidth   
99 25 moveto 
/Times-Roman findfont 9 scalefont setfont
(Operated by the Association of Universities for ) show
(Research in Astronomy, Inc., for the National ) show
(Aeronautics and Space Administration) show 
restore} def

